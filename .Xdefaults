
xterm.termName:  rxvt-256color
xterm.loginShell:         true
xterm.scrollBar:         false
xterm.internalBorder:        0
xterm.borderWidth:           0
xterm.secondaryScroll:    true
xterm.saveLines:         65535
xterm.cursorBlink:        true

! Notifications for pine and irssi under awesome
xterm.urgentOnBell:       true
!
! Extensions
xterm*perl-lib:         /usr/lib/urxvt/perl/
xterm*perl-ext-common:  default,matcher,searchable-scrollback
xterm*urlLauncher:      /usr/local/bin/firefox
xterm*matcher.button:   1
! So we catch even nasty URLs
xterm.cutchars:         `()'*<>[]{|}"
xtem*cutchars:         `"()'*<>[]{|}
!
! Appearance
xterm*font: -*-fixed-*-*-*-*-10-*-*-*-*-*-*-2
xterm*boldFont: -*-fixed-*-*-*-*-10-*-*-*-*-*-*-2

!xterm.font:     -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
!urxvt.boldFont: -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
!urxvt.font:     xft:Dina:style=Regular:size=10
!urxvt.boldFont: xft:Dina:style=Bold:size=10
!
xterm.shading:     30
xterm.cursorColor: #FFFFFF
! Pseudo Transparency
xterm.transparent: false
! Real transparency (with composite and xcompmgr)
!xtermTrans*depth:      32
!xtermTrans*background: rgba:0000/0000/0000/dddd
!xtermTrans*tintColor:  grey


xterm*background: #000000
xterm*foreground: #ffffff

xterm.keysym.Control-q: perl:mark-yank-urls:activate_mark_mode
xterm.underlineURLs: true
xterm.perl-ext: selection,mark-yank-urls
xterm.urlLauncher: firefox


!xterm --perl-lib ${HOME}/.urxvt/ -pe mark-yank-urls



